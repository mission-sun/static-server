<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title><%= users.title %></title>
    <style></style>
  </head>
  <body>
    <ul>
      <% users.files.forEach(function(user){ %>
      <li>
        <a href="<%= users.dir %>/<%= user %>">
          <%= user %>
        </a>
      </li>
      <% }) %>
    </ul>
  </body>
</html>
